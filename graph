import turtle as t
import time
print(t.screensize())

t1 = t.Turtle()

def drawAxis():
    t1.penup()
    t1.goto(0,300);
    t1.pendown(); t1.goto(0,0); t1.goto(300,0);
    t1.penup();
    

def g(x):
    t1.penup();
    y = 2*x;    t1.goto(x, y);
    t1.dot(5)
    
drawAxis();

for iv in range(0,150,10):
    g(iv)




